{
	"compilerOptions": {
		"target": "es6",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"paths": {
			"common/*": ["common/*"],
			"api/*": ["api/*"],
			"assets/*": ["assets/*"],
			"domains/*": ["domains/*"]
		},
		"baseUrl": "./src"
	},
	"include": ["src/**/*.ts", "src/**/*.js", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/**/*.png", "index.ts", ".eslintrc.js", "examples/old/InvestmentTypesPage.tsx"]
}
